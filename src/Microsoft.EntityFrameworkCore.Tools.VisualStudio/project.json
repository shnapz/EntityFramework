{
  "version": "1.0.0-preview3-*",
  "description": "Visual Studio commands for Entity Framework Core. Includes the Scaffold-DbContext, Add-Migration, and Update-Database commands for the Package Manager Console in Visual Studio.",
  "packOptions": {
    "tags": [
      "Entity Framework Core",
      "entity-framework-core",
      "EF",
      "Data",
      "O/RM"
    ],
    "files": [
      "lib/",
      "tools/"
    ]
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true,
    "compile": {
      "include": "../Shared/*.cs"
    }
  },
  "dependencies": {
    "Microsoft.EntityFrameworkCore.Design.Core": "1.0.0-*"
  },
  "frameworks": {
    "net451": {}
  }
}